{
    "app_dir"       : "./",
    "app_host"      : "localhost",
    "app_port"      : 3000,
    "db_dir"        : "./",
    "db_name"       : "db.sqlite3",
    "is_test"       : false,
    "dependencies"  : {},
    "statics"       : {
        "/static/dis"   : "../../extern/distrib-js",
        "/static/gws"   : "../gws-js/static/gws",
        "/static/pub-gws"   : "./public/static"
    },
    "app":{
        "favicon": {"src":"/static/pub/favicon.png", "type":"image/png"},
        "title": "GWS",
        "scripts": [
            "/static/dis/jquery/3.5.1/jquery.slim.min.js",
            "/static/dis/popper/2.4.2/popper.min.js",
            "/static/dis/bootstrap/4.5.0/js/bootstrap.min.js",
            "/static/dis/golden-layout/1.5.9/goldenlayout.js",
            "/static/dis/uuidv4/8.1.0/uuidv4.js",
            "/static/dis/fontawesome/5.13.1/all.min.js"
        ],
        "modules": [
            "/static/pub-gws/index.js"
        ],
        "styles": [
            "/static/dis/bootstrap/4.5.0/css/bootstrap.min.css",
            "/static/gws/dashboard/all.css",
            "/static/gws/dashboard/base-theme.css",
            "/static/gws/dashboard/dark-theme.css"
        ],
        "left_buttons":[
            { "name": "explorer", "icon": "fas fa-copy", "onclick": "window.gws.explorer" },
            { "name": "collab", "icon": "fas fa-users", "onclick": "window.gws.collab" },
            { "name": "biox", "icon": "fas fa-vials", "onclick": "window.gws.biox" },
            { "name": "biotadb", "icon": "fas fa-database", "onclick": "window.gws.biotadb" },
            { "name": "code", "icon": "fas fa-code", "onclick": "window.gws.code" },
            { "name": "settings", "icon": "fas fa-cog", "onclick": "window.gws.settings" }
        ]
    }
}